{
    "name": "fjbender/mollie-webhook-multiplexer",
    "description": "A simple webhook multiplexer for Mollie",
    "keywords": [
        "mollie",
        "webhook"
    ],
    "homepage": "http://github.com/fjbender/mollie-webhook-multiplexer",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Florian Bender",
            "email": "florian.bender@mollie.com",
            "homepage": "https://fbender.de"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "monolog/monolog": "^2.3",
        "nette/neon": "^3.2",
        "php-di/php-di": "^6.3",
        "psr/http-client": "^1.0",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.8"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.94",
        "phpunit/phpunit": "^9.5.8"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
